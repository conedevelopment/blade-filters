{
    "name": "thepinecode/blade-filters",
    "description": "Use filters easily in your blade templates.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerg≈ë D. Nagy",
            "email": "hello@iamgergo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pine\\BladeFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pine\\BladeFilters\\Tests\\": "tests/"
        }
    },
    "require": {
        "php" : "^7.0"
    },
    "require-dev": {
        "orchestra/testbench":"~3.8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pine\\BladeFilters\\BladeFiltersServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
